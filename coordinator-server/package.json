    "socket.io": "^2.3.0",
    "wdio-chromedriver-service": "^5.0.2",
    "webdriver": "^5.16.7",
    "webdriverio": "^5.16.7"
  },
  "engines": {
    "node": ">=8.15.1",
    "npm": ">=6.4.1"
  },
